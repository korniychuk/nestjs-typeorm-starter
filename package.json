{
  "name": "nest-typescript-starter",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "private": true,
  "version": "1.0.0",
  "bugs": {
    "url": "https://github.com/korniychuk/wallaby-ts-webpack-starter/issues"
  },
  "homepage": "https://github.com/korniychuk/wallaby-ts-webpack-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/korniychuk/wallaby-ts-webpack-starter.git"
  },
  "engines": {
    "node": "^12.14.1",
    "npm": "^6.13.4",
    "yarn": "^1.21.1"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:prod": "npm run build",
    "lint": "eslint --ext .js,.ts src/**",
    "lint:fix": "npm run lint -- --fix",
    "pre-push": "npm run lint && npm run test && npm run test:e2e && npm run build:prod && npm run tpl-repo:check",
    "prebuild": "rimraf dist",
    "preinstall": "node ./tools/check-yarn.js",
    "start": "ts-node src/main.ts",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/src/main.js\"",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/src/main.js\"",
    "start:prod": "node dist/src/main.js",
    "test": "jest --config jest.config.js",
    "test:cov": "npm run test -- --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.js",
    "test:e2e": "jest --config ./test/jest-e2e.config.js",
    "test:watch": "npm run test -- --watch",
    "tpl-repo": "tools/merge-with-repository-template.sh",
    "tpl-repo:check": "npm run tpl-repo -- check",
    "tpl-repo:merge": "npm run tpl-repo -- merge",
    "orm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrations:diff": "npm run orm -- migration:generate --name",
    "migrations:run": "npm run orm -- migration:run",
    "migrations:revert": "npm run orm -- migration:revert",
    "seeds:new": "ts-node src/configs/seed-generator.ts generate",
    "seeds:run": "npm run orm -- migration:run --connection 'seeding'",
    "seeds:revert": "npm run orm -- migration:revert --connection 'seeding'"
  },
  "dependencies": {
    "@nestjs/common": "~6.11.4",
    "@nestjs/config": "~0.1.0",
    "@nestjs/core": "~6.11.4",
    "@nestjs/platform-express": "~6.11.4",
    "@nestjs/typeorm": "~6.2.0",
    "@z-brain/typeorm-postgres-camelcase-naming-strategy": "~1.0.7",
    "dotenv": "~8.2.0",
    "lodash": "~4.17.21",
    "module-alias": "~2.2.2",
    "pg": "~7.17.1",
    "pluralize": "~8.0.0",
    "ramda": "~0.26.1",
    "reflect-metadata": "~0.1.13",
    "rimraf": "~3.0.0",
    "rxjs": "~6.5.3",
    "typeorm": "~0.2.22"
  },
  "devDependencies": {
    "@nestjs/cli": "~6.14.1",
    "@nestjs/schematics": "~6.9.1",
    "@nestjs/testing": "~6.11.4",
    "@types/express": "~4.17.1",
    "@types/jest": "~25.1.0",
    "@types/lodash": "~4.14.149",
    "@types/module-alias": "~2.0.0",
    "@types/node": "~12.7.8",
    "@types/pluralize": "~0.0.29",
    "@types/ramda": "~0.26.40",
    "@types/supertest": "~2.0.8",
    "@typescript-eslint/eslint-plugin": "~2.18.0",
    "@typescript-eslint/parser": "~2.18.0",
    "eslint": "~6.8.0",
    "eslint-config-airbnb-base": "~14.0.0",
    "eslint-import-resolver-typescript": "~2.0.0",
    "eslint-plugin-filenames": "~1.3.2",
    "eslint-plugin-import": "~2.20.0",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-unicorn": "~15.0.1",
    "husky": "~3.0.8",
    "jest": "~25.1.0",
    "jest-extended": "~0.11.5",
    "supertest": "~4.0.2",
    "ts-jest": "~25.0.0",
    "ts-loader": "~6.2.0",
    "ts-node": "~8.6.2",
    "tsc-watch": "~4.1.0",
    "typescript": "~3.7.5",
    "yargs": "~15.1.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run pre-push",
      "post-merge": "IS_YARN=true yarn install"
    }
  }
}
